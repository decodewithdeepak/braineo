# Appwrite Settings
# Base URL for Appwrite API endpoints
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
# Braineo project identifier in Appwrite
VITE_APPWRITE_PROJECT_ID=your_appwrite_project_id
# Database ID where all collections are stored
VITE_APPWRITE_DATABASE_ID=your_appwrite_database_id
# Collection IDs for different data schemas
VITE_USERS_COLLECTION_ID=users                   # Stores user profiles, auth data, and preferences
VITE_CAREER_PATHS_COLLECTION_ID=career-paths     # Stores learning paths with modules and progress
VITE_ASSESSMENTS_COLLECTION_ID=assessments       # Stores quiz results and user assessment data

# AI Models API Keys
# Google's Gemini AI - Used for generating learning content and module summaries
VITE_GEMINI_API_KEY=your_gemini_api_key
# Groq API - Used for faster AI responses in chat interactions
VITE_GROQ_API_KEY=your_groq_api_key

# External API Keys
# YouTube Data API - Used to fetch relevant educational videos for modules
VITE_YT_API_KEY=your_youtube_api_key
# Serper API - Google search results API for gathering learning resources
VITE_SERPER_API_KEY=your_serper_api_key